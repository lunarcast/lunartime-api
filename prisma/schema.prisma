generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Entry {
    ID    Int      @id @default(autoincrement()) @map("id")
    Name  String   @unique @map("name")
    Score Int      @map("score")
    Date  DateTime @map("date")

    @@map("entries")
}
